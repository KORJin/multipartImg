<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	<mapper namespace="com.java.aboutjeju.mapper.aboutMapper">
	<resultMap type="com.java.aboutjeju.dto.AboutjejuDto" id="aboutDto">
		<result column="LMnumber" property="LMnumber" />
		<result column="LMcategory" property="LMcategory" />
		<result column="LMtitle" property="LMtitle" />
		<result column="LMcontent" property="LMcontent" />
		<result column="LMaddress" property="LMaddress" />
		<result column="LMpostalcode" property="LMpostalcode"/>
		<result column="LMlatitude" property="LMlatitude" />
		<result column="LMlongitude" property="LMlongitude" />
	</resultMap>
	
	<insert id="insert" parameterType="com.java.aboutjeju.dto.AboutjejuDto">
		INSERT INTO Landmark 
		VALUES(Landmark_LMnumber_seq.nextval,#{LMcategory},#{LMtitle},#{LMcontent},#{LMaddress},#{LMlatitude}.#{LMlongitude})
	</insert>
</mapper>